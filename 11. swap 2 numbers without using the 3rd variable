#1. Using Arithmetic Operations

def swap_numbers(a, b):
    a = a + b
    b = a - b
    a = a - b
    return a, b


num1 = 5
num2 = 10
num1, num2 = swap_numbers(num1, num2)
print("After swapping: num1 = ", num1, ", num2 = ", num2)

#2. Using Bitwise XOR Operation

def swap_numbers(a, b):
    a = a ^ b
    b = a ^ b
    a = a ^ b
    return a, b


num1 = 5
num2 = 10
num1, num2 = swap_numbers(num1, num2)
print("After swapping: num1 =", num1, ", num2 =", num2)
